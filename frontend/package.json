{
  "name": "pnku-website-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && cross-env GENERATE_SOURCEMAP=true vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier:fix": "prettier --write .",
    "prettier:check": "prettier --check .",
    "check": "pnpm lint && pnpm prettier:check && pnpm build",
    "analyze": "source-map-explorer --gzip --no-border-checks 'build/assets/*.js'",
    "clean": "rimraf build"
  },
  "dependencies": {
    "@ant-design/colors": "^7.1.0",
    "@ant-design/icons": "^5.4.0",
    "@ant-design/plots": "^1.2.6",
    "antd": "^5.20.5",
    "colorjs.io": "^0.5.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "html-react-parser": "^5.1.15",
    "react": "^18.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.1",
    "react-timeago": "^7.2.0",
    "react-use-error-boundary": "^3.0.0",
    "seedrandom": "^3.0.5",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.5",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.3.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-timeago": "^4.1.7",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "daisyui": "^3.9.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "javascript-obfuscator": "^4.1.1",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "rimraf": "~5.0.10",
    "rollup-plugin-obfuscator": "^1.1.0",
    "sass": "^1.78.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.10",
    "terser": "^5.31.6",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-compression2": "^1.2.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.2.0"
  }
}
